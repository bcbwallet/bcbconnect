{
  "manifest_version": 2,
  "key": "",
  "name": "BCB Connect",
  "version": "2.6.1",
  "description": "A BCB wallet in your browser",
  "author": "BCB Community",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
      "storage"
  ],
  "browser_action": {
      "default_popup": "packages/popup/dist/index.html",
      "default_title": "BCB Connect",
      "default_icon": "images/icon-48.png"
  },
  "icons": {
    "128": "images/icon-48.png"
  },
  "background": {
      "scripts": [
          "dist/backgroundScript.js"
      ],
      "persistent": true
  },
  "content_scripts": [
      {
          "matches": [
              "http://*/*",
              "https://*/*"
          ],
          "js": [
              "dist/contentScript.js"
          ],
          "run_at": "document_start",
          "all_frames": true
      }
  ],
  "web_accessible_resources": [
      "dist/inPageScript.js"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Alt+Shift+N",
        "linux": "Alt+Shift+N",
        "mac": "Alt+Shift+N",
        "windows": "Alt+Shift+N"
     }
    }
  }
}
